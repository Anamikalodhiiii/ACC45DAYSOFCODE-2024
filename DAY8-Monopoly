#include <iostream>
using namespace std;

int main() {
    int T;
    cin >> T;

    while (T--) {
        int P, Q, R, S;
        cin >> P >> Q >> R >> S;

        int sum = P + Q + R + S;

        if (P > sum - P || Q > sum - Q || R > sum - R || S > sum - S) {
            cout << "YES" << endl;
        } else {
            cout << "NO" << endl;
        }
    }

    return 0;
}
