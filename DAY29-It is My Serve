#include <iostream>
using namespace std;

string whoseServe(int p, int q) {
    int totalPoints = p + q;
    return (totalPoints % 2 == 0) ? "Alice" : "Bob";
}

int main() {
    int t;  
    cin >> t;

    while (t--) {
        int p, q;  
        cin >> p >> q;

        cout << whoseServe(p, q) << endl;
    }

    return 0;
}
