#include <iostream>
#include <string>
using namespace std;

int main() {
    int t;  
    cin >> t;

    while (t--) {
        int n;  
        cin >> n;

        int start38 = 0;  
        int ltime108 = 0;  

        for (int i = 0; i < n; i++) {
            string contest;
            cin >> contest;

            if (contest == "START38") {
                start38++;
            } else if (contest == "LTIME108") {
                ltime108++;
            }
        }

        cout << start38 << " " << ltime108 << endl;
    }

    return 0;
}

